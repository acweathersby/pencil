    "semanticHighlighting": false,
    "semanticTokenColors": {
        "union.declaration": "#e6662f",
        "class": "#5d97b4",
        "function.declaration": "#e6662f",
        "keyword": "#7c7c7c",
        "punctuation.brackets": "#ff0000"
    },
    "tokenColors": [
        {
            "name": "Base",
            "scope": [
                "source"
            ],
            "settings": {
                "foreground": "#7c7c7c",
            }
        },
        /*
        ██   ██ ███████ ██    ██ ██     ██  ██████  ██████  ██████  ███████ 
        ██  ██  ██       ██  ██  ██     ██ ██    ██ ██   ██ ██   ██ ██      
        █████   █████     ████   ██  █  ██ ██    ██ ██████  ██   ██ ███████ 
        ██  ██  ██         ██    ██ ███ ██ ██    ██ ██   ██ ██   ██      ██ 
        ██   ██ ███████    ██     ███ ███   ██████  ██   ██ ██████  ███████
        */
        {
            "name": "Base Keywords",
            "scope": [
                "constant.language.json",
                "keyword",
                "meta.function",
                "meta.tag.metadata.doctype.html",
                "punctuation.brackets.curly.rust",
                "punctuation.definition.block",
                "punctuation.definition.parameters",
                "punctuation.section.block",
                "storage",
            ],
            "settings": {
                "foreground": "#7c7c7c",
                "fontStyle": ""
            }
        },
        {
            "name": "Labels",
            "scope": [
                "entity.name.label",
                "storage.type.modifier.access.control"
            ],
            "settings": {
                "foreground": "#7c7c7c",
            }
        },
        {
            "name": "CSS Keyword Values",
            "scope": [
                "support.constant.property-value.css",
                "support.constant.color"
            ],
            "settings": {
                "foreground": "#7c7c7c",
                "fontStyle": "bold"
            }
        },
        /*
        ██    ██  █████  ██████  ██  █████  ██████  ██      ███████ ███████ 
        ██    ██ ██   ██ ██   ██ ██ ██   ██ ██   ██ ██      ██      ██      
        ██    ██ ███████ ██████  ██ ███████ ██████  ██      █████   ███████ 
         ██  ██  ██   ██ ██   ██ ██ ██   ██ ██   ██ ██      ██           ██ 
          ████   ██   ██ ██   ██ ██ ██   ██ ██████  ███████ ███████ ███████ 
        */
        {
            "name": "Global Variables",
            "scope": [
                "entity.name.namespace",
                "entity.name.scope-resolution",
                "punctuation.separator.scope-resolution",
                "support.constant.math",
                "variable.language.special.self.python",
                "variable.language.super",
                "source.shell"
            ],
            "settings": {
                "foreground": "#7c7c7c",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Variables",
            "scope": [
                "variable.language",
                "entity.name.tag.css",
                "meta.body",
                "meta.function-call.arguments",
                "new.expr",
                "variable.other",
                "variable.parameter",
                "entity.name.type.enum",
                "entity.name.label",
                "entity.other.attribute-name.html",
                "entity.other.attribute-name",
                "literal.key",
                "meta.attribute.html",
                "meta.definition.variable",
                "meta.object-literal.key",
                "meta.object.member",
                "support.type.property-name.css",
                "meta.method.declaration",
                "constant.other.caps"
            ],
            "settings": {
                "foreground": "#616161",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Keys & Members",
            "scope": [
                "entity.name.function.member",
                "entity.other.attribute-name.id.css",
                "markup.list",
                "meta.attribute.python",
                "meta.attribute.rust",
                "meta.member.access",
                "meta.object.member",
                "support.variable.property",
                "variable.other.constant.property",
                "variable.other.object.property",
                "variable.other.property",
                "variable.other.enummember",
                "variable.object.property",
                "meta.definition.property"
            ],
            "settings": {
                "foreground": "#333333",
                "fontStyle": ""
            }
        },
        {
            "name": "Calls",
            "scope": [
                "entity.name.function",
                "entity.name.function.macro",
                "entity.name.function.call",
                "meta.function-call",
                "meta.function.call",
                "markup.quote.markdown",
            ],
            "settings": {
                "foreground": "#333333",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Block Declarations",
            "scope": [
                "entity.name.function.constructor",
                "entity.name.function.definition",
                "entity.name.module",
                "entity.name.type.class",
                "keyword.other.operator.overload",
                "meta.function.definition.rust",
                "meta.definition.function",
                "meta.definition.method",
                "meta.head.function.definition.special.operator-overload",
                "support.type.property-name.json",
            ],
            "settings": {
                "foreground": "#7c7c7c",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Types & Storage",
            "scope": [
                "cast.expr",
                "entity.name.type",
                "entity.other.inherited-class",
                "fenced_code.block.language.markdown",
                "markup.heading.markdown",
                "meta.arguments.decltype",
                "meta.return.type",
                "meta.template.definition",
                "meta.type",
                "keyword.operator.dereference",
                "keyword.operator.borrow",
                "storage.type.built-in.primitive",
                "storage.type.primitive",
                "storage.type.template.argument",
                "storage.type",
                "storage.modifier",
                "keyword.other.rust",
                "support.function.builtin"
            ],
            "settings": {
                "foreground": "#8961d5",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Defined Types",
            "scope": [
                "meta.tag",
            ],
            "settings": {
                "foreground": "#7c7c7c",
                "fontStyle": "italic"
            }
        },
        {
            "name": "CSS Tag Selector",
            "scope": [
                "entity.name.tag.css"
            ],
            "settings": {
                "foreground": "#7c7c7c",
                "fontStyle": "italic"
            }
        },
        {
            "name": "CSS Class Selector",
            "scope": [
                "entity.other.attribute-name.class.css"
            ],
            "settings": {
                "foreground": "#7c7c7c",
                "fontStyle": "bold"
            }
        },
        {
            "name": "CSS Meta Selector",
            "scope": [
                "meta.selector.css"
            ],
            "settings": {
                "foreground": "#7c7c7c",
                "fontStyle": "italic"
            }
        },
        /*
        ███████ ████████ ██████  ██ ███    ██  ██████  ███████ 
        ██         ██    ██   ██ ██ ████   ██ ██       ██      
        ███████    ██    ██████  ██ ██ ██  ██ ██   ███ ███████ 
             ██    ██    ██   ██ ██ ██  ██ ██ ██    ██      ██ 
        ███████    ██    ██   ██ ██ ██   ████  ██████  ███████ 
        */
        {
            "name": "Strings",
            "scope": [
                "string",
                "source.ignore",
                "punctuation.definition.string",
                "markup.underline.link.image.markdown",
                "meta.link.inline.markdown"
            ],
            "settings": {
                "foreground": "#599575",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Raw String",
            "scope": [
                "string.unquoted",
                "markup.inline",
            ],
            "settings": {
                "foreground": "#7c7c7c",
                "fontStyle": "italic"
            }
        },
        /*
        ██      ██ ████████ ███████ ██████   █████  ██      ███████ 
        ██      ██    ██    ██      ██   ██ ██   ██ ██      ██      
        ██      ██    ██    █████   ██████  ███████ ██      ███████ 
        ██      ██    ██    ██      ██   ██ ██   ██ ██           ██ 
        ███████ ██    ██    ███████ ██   ██ ██   ██ ███████ ███████
        */
        {
            "name": "Numeric Literal",
            "scope": [
                "constant.language",
                "constant.numeric",
                "keyword.other.unit",
                "meta.structure.dictionary.value.json"
            ],
            "settings": {
                "foreground": "#5a80da",
                "fontStyle": ""
            }
        },
        {
            "name": "Object & Array Literals",
            "scope": [
                "meta.array.literal",
                "meta.objectliteral",
            ],
            "settings": {
                "foreground": "#7c7c7c",
                "fontStyle": "bold"
            }
        },
        /*
         ██████  ██████  ███    ███ ███    ███ ███████ ███    ██ ████████ ███████ 
        ██      ██    ██ ████  ████ ████  ████ ██      ████   ██    ██    ██      
        ██      ██    ██ ██ ████ ██ ██ ████ ██ █████   ██ ██  ██    ██    ███████ 
        ██      ██    ██ ██  ██  ██ ██  ██  ██ ██      ██  ██ ██    ██         ██ 
         ██████  ██████  ██      ██ ██      ██ ███████ ██   ████    ██    ███████
        */ {
            "name": "Comments",
            "scope": [
                "comment",
                "punctuation.definition.comment",
                "markup.heading.setext"
            ],
            "settings": {
                "foreground": "#9a9a9a",
                "fontStyle": "italic"
            }
        },
        /*
         ██████  ██████  ███████ ██████   █████  ████████  ██████  ██████  ███████ 
        ██    ██ ██   ██ ██      ██   ██ ██   ██    ██    ██    ██ ██   ██ ██      
        ██    ██ ██████  █████   ██████  ███████    ██    ██    ██ ██████  ███████ 
        ██    ██ ██      ██      ██   ██ ██   ██    ██    ██    ██ ██   ██      ██ 
         ██████  ██      ███████ ██   ██ ██   ██    ██     ██████  ██   ██ ███████ 
        */
        {
            "name": "Operator",
            "scope": [
                "constant.language.import-export-all.js",
                "keyword.operator",
                "operator",
                "storage.type.function.arrow",
                "punctuation.definition.variable"
            ],
            "settings": {
                "foreground": "#939393",
                "fontStyle": "italic bold",
            }
        },
        /*
        ██████  ██    ██ ███    ██  ██████ ████████ ██    ██  █████  ████████ ██  ██████  ███    ██ 
        ██   ██ ██    ██ ████   ██ ██         ██    ██    ██ ██   ██    ██    ██ ██    ██ ████   ██ 
        ██████  ██    ██ ██ ██  ██ ██         ██    ██    ██ ███████    ██    ██ ██    ██ ██ ██  ██ 
        ██      ██    ██ ██  ██ ██ ██         ██    ██    ██ ██   ██    ██    ██ ██    ██ ██  ██ ██ 
        ██       ██████  ██   ████  ██████    ██     ██████  ██   ██    ██    ██  ██████  ██   ████
        */
        {
            "name": "Punctuation",
            "scope": [
                "keyword.control.directive",
                "markup",
                "meta.arrow",
                "meta.class",
                "meta.method.declaration",
                "meta.object-binding-pattern-variable.ts",
                "punctuation",
            ],
            "settings": {
                "foreground": "#9c9c9c",
                "fontStyle": ""
            }
        },
        {
            "name": "Punctuation Member Accessor",
            "scope": [
                "punctuation.accessor",
                "punctuation.separator.key-value",
            ],
            "settings": {
                "foreground": "#7c7c7c",
                "fontStyle": "bold"
            }
        },
        /*
        ██████  ██████   █████   ██████ ██   ██ ███████ ████████ ███████ 
        ██   ██ ██   ██ ██   ██ ██      ██  ██  ██         ██    ██      
        ██████  ██████  ███████ ██      █████   █████      ██    ███████ 
        ██   ██ ██   ██ ██   ██ ██      ██  ██  ██         ██         ██ 
        ██████  ██   ██ ██   ██  ██████ ██   ██ ███████    ██    ███████
        */
        {
            "name": "HTML Brackets",
            "scope": [
                "punctuation.definition.tag",
                "punctuation.section.property-list"
            ],
            "settings": {
                "foreground": "#7c7c7c",
                "fontStyle": "bold"
            }
        },
        {
            "name": "String Template Brackets",
            "scope": [
                "punctuation.definition.template-expression",
            ],
            "settings": {
                "foreground": "#7c7c7c",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Brackets",
            "scope": [
                "meta.brace.css",
                "punctuation.section.arguments.begin.bracket",
                "punctuation.section.arguments.end.bracket",
                "punctuation.section.parameters.begin.bracket",
                "punctuation.section.parameters.end.bracket",
            ],
            "settings": {
                "foreground": "#7c7c7c",
                "fontStyle": "italic"
            }
        },
        /*
         ██████  ████████ ██   ██ ███████ ██████  
        ██    ██    ██    ██   ██ ██      ██   ██ 
        ██    ██    ██    ███████ █████   ██████  
        ██    ██    ██    ██   ██ ██      ██   ██ 
         ██████     ██    ██   ██ ███████ ██   ██
        */
        {
            "name": "Plain Text",
            "scope": [
                "text",
                "text.html.markdown",
                "text.html.derivative",
            ],
            "settings": {
                "foreground": "#7c7c7c",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Bold",
            "scope": [
                "markup.bold.markdown",
                "punctuation.definition.bold.markdown"
            ],
            "settings": {
                "foreground": "#7c7c7c",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Italics",
            "scope": [
                "markup.italic",
            ],
            "settings": {
                "foreground": "#7c7c7c",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Links",
